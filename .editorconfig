; top-most EditorConfig file
root = true

; Unix-style newlines with a newline ending every file + tab indent style with 4 space indentation
[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
indent_style = tab
indent_size = 4

[{package.json,bower.json}]
indent_style = space
indent_size = 2
